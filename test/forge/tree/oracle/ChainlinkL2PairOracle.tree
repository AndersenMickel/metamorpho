.
└── constructor(address scaleFactor, address collateralFeed, address borrowableFeed, address sequencerUptimeFeed, uint256 gracePeriod)
    ├── SCALE_FACTOR = scaleFactor
    ├── COLLATERAL_FEED = (CHAINLINK_V3, collateralFeed)
    ├── COLLATERAL_SCALE = 10 ** collateralFeed.decimals()
    ├── BORROWABLE_FEED = (CHAINLINK_V3, borrowableFeed)
    └── BORROWABLE_SCALE = 10 ** borrowableFeed.decimals()

.
└── borrowablePrice()
    └── return ChainlinkL2CollateralAdapter(borrowableFeed).borrowablePrice()

.
└── collateralPrice()
    └── return ChainlinkL2CollateralAdapter(collateralFeed).collateralPrice()

.
└── price()
    ├── if sequencerUptimeFeed.answer != 0 || block.timestamp - sequncerUptimeFeed <= gracePeriod
    │   └── revert
    └── else
        └── return scaleFactor * collateralPrice() * (10 ** borrowableFeed.decimals()) / borrowablePrice() / (10 ** collateralFeed.decimals())
